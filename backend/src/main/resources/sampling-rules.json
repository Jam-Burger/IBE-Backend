{
  "version": 2,
  "rules": [
    {
      "description": "Critical paths",
      "host": "*",
      "http_method": "*",
      "url_path": "/api/*/bookings/*",
      "fixed_target": 1,
      "rate": 1.0,
      "priority": 1
    },
    {
      "description": "Default rule",
      "host": "*",
      "http_method": "*",
      "url_path": "/*",
      "fixed_target": 1,
      "rate": 0.05,
      "priority": 1000
    }
  ],
  "default": {
    "fixed_target": 1,
    "rate": 0.05
  }
} 